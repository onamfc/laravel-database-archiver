{
    "name": "onamfc/laravel-database-archiver",
    "version": "1.0.2",
    "description": "A Laravel package for efficient database record archival to cold storage",
    "keywords": ["laravel", "database", "archival", "cold-storage", "s3", "parquet"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Brandon Estrella",
            "email": "brandonestrelladev@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/console": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "aws/aws-sdk-php": "^3.0",
        "league/flysystem-aws-s3-v3": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelDbArchiver\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelDbArchiver\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelDbArchiver\\Providers\\DbArchiverServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}